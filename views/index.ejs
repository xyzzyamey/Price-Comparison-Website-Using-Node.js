            <img class="card-img-top" src="images\flp.jpg" alt="Card image cap">
            <a href="https://www.flipkart.com">
              <div class="mask rgba-white-slight"></div>
            </a>
          </div>
          <!--Card image-->

          <!--Card content-->
          <div class="card-body">
            <!--Title-->
            <a>
