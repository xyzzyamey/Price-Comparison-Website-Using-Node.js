            <!--Text-->
            <p class="card-text">

            <% flp_str="" %>
            <% for(i=0;i<flipkart.length;i++){ %>
                <% if(flipkart[i].item==undefined || flipkart[i].price==undefined){flipkart[i].item=" ";flipkart[i].price=" ";continue;} %>
                <% flp_str+=`${flipkart[i].item} <span class='float-right' > ${flipkart[i].price} </span><br><br>` %>
            <% } %>
            <%- flp_str %>
            <hr><a href= <%= links[0] %>><button type="button" class="btn btn-primary">Visit Flipkart</button></a>

